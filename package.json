{
  "name": "kalh",
  "version": "1.0.0",
  "description": "Landing page for komo academy learning hub",
  "main": "index.js",
  "scripts": {
      "watch:sass": "node-sass sass/main.scss css/style.css -w",
      "devserver": "live-server",
      "start": "npm-run-all parallel devserver watch:sass",
      
      "compile:sass": "node-sass sass/main.scss css/style.comp.css",
      "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
      "prefix:css": "postcss --use autoprefixer -b 'last 10 version' css/style.concat.css -o css/style.prefixer.css",
      "compress:css": "node-sass css/style.prefixer.css css/style.css --output-style compressed",
      "build:css": "npm-run-all compile:sass concat:css prefixer:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/komoacademy/kalh.git"
  },
  "keywords": [
    "learning",
    "hub",
    "education",
    "training",
    "school",
    "center",
    "class",
    "komo academy"
  ],
  "author": "Geraldo Kandonga Fillipus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/komoacademy/kalh/issues"
  },
  "homepage": "https://github.com/komoacademy/kalh#readme",
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "concat": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  }
}
